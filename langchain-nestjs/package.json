{
  "name": "langchain-learn-path-nestjs",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "src",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production rm -rf dist && yarn build && node dist/src/main",
    "lint": "eslint --config .eslint.mjs",
    "test": "jest",
    "patch-version": "npm version patch --no-git-tag-version"
  },
  "dependencies": {
    "@langchain/community": "^1.1.0",
    "@langchain/core": "^1.1.5",
    "@langchain/ollama": "^1.1.0",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/sequelize": "^11.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/testing": "^11.1.9",
    "assert": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "ioredis": "^5.8.2",
    "langchain": "^1.1.3",
    "lodash": "^4.17.21",
    "mongoose": "^9.0.1",
    "nestjs-pino": "^4.5.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "typescript-eslint": "^8.48.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
